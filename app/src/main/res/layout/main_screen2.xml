<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginBottom="15dp"
        android:layout_weight="10" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:orientation="horizontal"
        android:layout_weight="10">
        <Button
            android:id="@+id/go_info"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/mbtn1"
            android:layout_weight="10"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="20dp" />

        <Button
            android:id="@+id/go_share"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/mbtn2"
            android:layout_weight="10"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="20dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:orientation="horizontal"
        android:layout_weight="10">

            <Button
                android:id="@+id/go_consult"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/mbtn3"
                android:layout_weight="10"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="20dp" />

            <Button
                android:id="@+id/go_map"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/mbtn4"
                android:layout_weight="10"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="20dp" />

        </LinearLayout>

    <Button
        android:id="@+id/go_board"
        android:layout_width="match_parent"
        android:background="@drawable/mbtn5"
        android:layout_height="130dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="50dp" />


</LinearLayout>