<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFC600"
    android:elevation="2.67dp">

    <View
        android:id="@+id/image_1"
        android:layout_width="158.67dp"
        android:layout_height="201.33dp"
        android:layout_marginLeft="161.33dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="185.33dp"
        android:background="@drawable/image_1"
        android:elevation="2.67dp"
        />

    <RelativeLayout
        android:id="@+id/frame_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="358.67dp"
        android:clipToOutline="true">

        <TextView
            android:id="@+id/pae"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/test"
            android:text="PAE"
            android:textAppearance="@style/pae" />

        <TextView
            android:id="@+id/some_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="68dp"
            android:gravity="top"
            android:lineSpacingExtra="23sp"
            android:text="이메일"
            android:textAppearance="@style/some_id" />

        <EditText
            android:id="@+id/email_0006"
            android:layout_width="match_parent"
            android:layout_height="53.33dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="93.33dp"
            android:layout_marginRight="16dp"
            android:paddingLeft="16dp"
            android:background="@drawable/rectangle_3"
            android:elevation="2.67dp"
            android:hint="test@test.com"
            android:textColor="#000000"
            android:textColorHint="#C0C0C0"
            android:textSize="15sp"
            tools:layout_editor_absoluteY="109dp" />

        <TextView
            android:id="@+id/some_id1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="173.33dp"
            android:gravity="top"
            android:lineSpacingExtra="23sp"
            android:text="비밀번호"
            android:textAppearance="@style/some_id" />

        <EditText
            android:id="@+id/password_0006"
            android:layout_width="match_parent"
            android:layout_height="53.33dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="198.67dp"
            android:layout_marginRight="16dp"
            android:background="@drawable/rectangle_3"
            android:elevation="2.67dp"
            android:hint="영문+숫자+특수문자 포함 8자 이상"
            android:paddingLeft="16dp"
            android:password="true"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textColorHint="#C0C0C0"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="330dp" />

        <TextView
            android:id="@+id/SignUp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="260dp"
            android:layout_marginEnd="20dp"
            android:layout_toStartOf="@id/femail"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="29sp"
            android:text="회원가입"
            android:textAppearance="@style/some_id" />

        <TextView
            android:id="@+id/femail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="260dp"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="29sp"
            android:text="이메일 찾기"
            android:textAppearance="@style/some_id" />

        <TextView
            android:id="@+id/fpassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="260dp"
            android:layout_toEndOf="@id/femail"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="29sp"
            android:text="비밀번호 찾기"
            android:textAppearance="@style/some_id" />

        <Button
            android:id="@+id/go_1001"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:elevation="2.5dp"
            android:layout_below="@id/femail"
            android:gravity="center_horizontal|center_vertical"
            android:text="로그인" />

    </RelativeLayout>


/>


</RelativeLayout>